{
  "manifest_version": 3,
  "name": "E-PIYUTools",
  "version": "2.0",
  "description": "Tools for FEU TECH Student Portal",
  "icons": {
    "128": "128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "EPIYUTools_Logo.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://students.feutech.edu.ph/accounts/*"],
      "js": ["content.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "epiyutools@chikiran"
    }
  }
}
